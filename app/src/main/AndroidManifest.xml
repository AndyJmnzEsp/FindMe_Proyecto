        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".HomeActivity"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".ResetContrasenia"></activity>
        <activity android:name=".SingInActivity"></activity>
        <activity android:name=".CheckInActivity"></activity>
        <activity android:name=".CheckInNormalActivity"></activity>
        <activity android:name=".CheckInPsicoUserActivity"></activity>
        <activity android:name=".CheckInPsicoActivity"></activity>
        <activity android:name=".AutentificacionPsicoActivity"></activity>
        <activity android:name=".VerificacionPsicoActivity"></activity>
        <activity android:name=".CheckInAdminUserActivity"></activity>
        <activity android:name=".CheckInAdminActivity"></activity>
        <activity android:name=".AutentificacionAdminActivity"></activity>
        <activity android:name=".VerificacionAdminActivity"></activity>
        <activity android:name=".AutentificacionActivity"></activity>
        <activity android:name=".VerificacionActivity"></activity>
        <activity android:name=".TerminosYCondicionesActivity"></activity>
        <activity android:name=".VerifyActivity"></activity>
        <activity android:name=".PrincipalMuroActivity"></activity>

    </application>


</manifest>